# 系统初始化器
org.springframework.context.ApplicationContextInitializer=\
tech.edwardvan.testspringbootautoconfigure.initializer.TestApplicationContextInitializer

# Auto Configure
org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
tech.edwardvan.testspringbootautoconfigure.config.TestAutoConfiguration

# 应用事件监听器
org.springframework.context.ApplicationListener=\
tech.edwardvan.testspringbootautoconfigure.listener.TestApplicationListener,\
tech.edwardvan.testspringbootautoconfigure.listener.BeforeConfigFileApplicationListener,\
tech.edwardvan.testspringbootautoconfigure.listener.AfterConfigFileApplicationListener

# 运行监听器
org.springframework.boot.SpringApplicationRunListener=\
tech.edwardvan.testspringbootautoconfigure.listener.TestSpringApplicationRunListener